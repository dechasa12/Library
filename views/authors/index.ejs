<h2 class="page-header"> Search Authors</h2>
<label>name</label>
<form action="/authors" method="GET">
    <fieldset>
        <input type="text" name="name" value="<%= searchOptions.name || '' %>">
        <button type="submit">Search</button>
    </fieldset>
</form>

<br>
<% authors.forEach(author => {%>
    <div><%= author.name %></div>
    <div class="btn-row">
    <a href="/authors/<%= author.id %>" class="btn btn-primary">View</a>
    <a href="/authors/<%= author.id %>/edit" class="btn btn-primary">Edit</a>
    <%-include('../partials/deleteForm.ejs',{url:`/authors/${author.id }` })%>
    <%})%>
</div>